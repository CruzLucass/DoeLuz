
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width , initial-scale=1, shrink-to-fit=no" />
    <link rel="stylesheet" asp-href-include="/lib/bootstrap/dist/**/*.min.css" />
    <title>Doe Luz</title>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark badge-danger" style="background-color: #ffffff;" role="navigation">
    <div class="container" style="text-align:center">
        <img src="/lib/bootstrap/js/src/logo.jpg" alt="" width="150" height="100">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSite">
            <span class="navbar-toggler-icon"></span>
        </button>
    </div>
    </nav>
    <div class="row m-1 p-1 ">
        <div id="categories" class="col-2">
        </div>
        <div class="col-9 justify-content-center">
            <h1 style="text-align:center"><b>Cadastro realizado com sucesso!</b></h1>
            <br />
            <h6 style="text-align:center">Você cadastrou um novo admistrador, clique no botão para voltar ao menu principal.</h6>
            <br />
            <hr />
            <div class="col-lg-12 justify-content-center">
                @Html.ActionLink("Voltar ao Menu", "Home", "SessionControler", null, new { @class = "btn btn-primary btn-lg btn-block btn-danger"})
            </div>
            <br />
        </div>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - Doe Luz - <a asp-area="" asp-controller="SessionControler" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>

